{
  "version": "1.0.0-*",

  "description": "Experimental, work-in-progress parser combinator library",
  "authors": [ "Datalust", "Superpower Contributors", "Sprache Contributors" ],

  "packOptions": {
    "tags": [ "superpower", "parser" ],
    "projectUrl": "https://github.com/nblumhardt/superpower",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0"
  },

  "buildOptions": {
    "keyFile": "../../asset/Superpower.snk",
    "warningsAsErrors": true
  },

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE", "CHECKED" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },

  "frameworks": {
    "netstandard1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    }
  }
}
